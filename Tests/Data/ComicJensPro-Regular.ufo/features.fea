languagesystem latn dflt;
languagesystem cyrl dflt;
languagesystem grek dflt;
languagesystem latn AZE;
languagesystem latn CRT;
languagesystem latn DEU;
languagesystem latn ENG;
languagesystem latn MOL;
languagesystem latn ROM;
languagesystem latn TRK;


feature aalt {
#------------------------------
# All Alternates
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    feature case;
    feature ordn;
    feature frac;
    feature liga;
    feature hlig;
    feature ss02;
    feature calt;
    feature salt;
    feature locl;
    feature hist;
    feature smcp;
    feature dlig;
    feature ss01;
    feature ornm;
} aalt;

feature locl {
#------------------------------
# Localized Forms
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
  script latn;
  # Latin
    language AZE;
    # Azeri
      sub i by i.latn_TRK;
    language CRT;
    # Crimean Tatar
      sub i by i.latn_TRK;
    language DEU;
    # German
      sub zero_seven by zero_seven.latn_DEU;
      sub seven by seven.latn_DEU;
    language ENG;
    # English
    	sub one by one.latn_ENG;
    language MOL;
    # Moldavian
      sub [Scedilla scedilla] by [Scommaaccent scommaaccent];
      sub [Tcommaaccent tcommaaccent] by [uni021A uni021B];
    language ROM;
    # Romanian
      sub [Scedilla scedilla] by [Scommaaccent scommaaccent];
      sub [Tcommaaccent tcommaaccent] by [uni021A uni021B];
    language TRK;
    # Turkish
      sub i by i.latn_TRK;
} locl;

feature calt {
#------------------------------
# Contextual Alternates
# <version number="2.02"/>
# <protection state="yes"/>
#------------------------------
    sub @uppercaseLetters @combMark_source' by @combMark_target;
    sub afii10026 afii10026' by afii10026.calt;
} calt;

feature frac {
#------------------------------
# Fractions
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub one @fractionSlashes four by onequarter;
    sub one @fractionSlashes two by onehalf;
    sub three @fractionSlashes four by threequarters;
} frac;

feature ordn {
#------------------------------
# Ordinals
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub [@ordnnumbers @ordn_target] @ordn_source' by @ordn_target;
} ordn;

feature hist {
#------------------------------
# Historical Forms
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub @hist_source by @hist_target;
} hist;

feature case {
#------------------------------
# Case-Sensitive Forms
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub @case_source by @case_target;
} case;

feature dlig {
#------------------------------
# Discretionary Ligatures
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub a a by a_a;
    sub c c by c_c;
    sub d d by d_d;
    sub e e by e_e;
    sub g g by g_g;
    sub l e by l_e;
    sub l l by l_l;
    sub m m by m_m;
    sub n n by n_n;
    sub o o by o_o;
    sub p p by p_p;
    sub r r by r_r;
    sub s s by s_s;
    sub t t by t_t;
    sub w w w by w_w_w;
    sub zero seven by zero_seven;
    sub zero zero by zero_zero;
} dlig;

feature liga {
#------------------------------
# Standard Ligatures
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub f f by uniFB00;
    sub f i by fi;
    sub f l by fl;
} liga;

feature hlig {
#------------------------------
# Historical Ligatures
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub longs i by longs_i;
    sub longs longs by longs_longs;
} hlig;

feature salt {
#------------------------------
# Stylistic Alternates
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub @ss01_source by @ss01_target;
} salt;

feature ss01 {
#------------------------------
# Stylistic Set 1
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub @ss01_source by @ss01_target;
} ss01;

feature ss02 {
#------------------------------
# Stylistic Set 2
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub @ss02_source by @ss02_target;
} ss02;

feature ss03 {
#------------------------------
# Stylistic Set 3
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
# clone of hist (Historical Forms)
    sub @hist_source by @hist_target;
} ss03;

feature ornm {
#------------------------------
# Ornaments
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    sub bullet from @ornaments;
} ornm;

feature cpsp {
#------------------------------
# Capital Spacing
# <version number="2.02"/>
# <protection state="no"/>
#------------------------------
    pos @uppercaseLetters <5 0 10 0>;
} cpsp;
